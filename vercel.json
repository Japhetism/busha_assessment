{
    "version": 2,
    "builds": [
      {
        "src": "build.sh",
        "use": "@vercel/static-build"
      },
      {
        "src": "server/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/index.html"
      }
    ]
  }
  