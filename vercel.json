{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",     
      "use": "@vercel/node"       
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",          
      "dest": "server/index.js"
    }
  ],
  "buildCommand": "npm install --prefix server && npm install && npm run build"
}