{
    "version": 2,
    "builds": [
      {
        "src": "package.json",        
        "use": "@vercel/react"        
      },
      {
        "src": "server/index.js",     
        "use": "@vercel/node"        
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",          
        "dest": "server/index.js"
      },
      {
        "src": "/(.*)",              
        "dest": "/$1"
      },
      {
        "src": "/",                 
        "dest": "/index.html"
      }
    ]
  }
  